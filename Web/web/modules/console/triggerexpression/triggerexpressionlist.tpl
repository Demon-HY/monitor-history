<div class="data-toolbar">
    <button class="btn btn-info add-triggerexpression">{{t 'consoleTriggerExpression_add_triggerexpression'}}</button>
    <button class="btn btn-info edit-triggerexpression">{{t 'consoleTriggerExpression_edit_triggerexpression'}}</button>
    <button class="btn btn-info delete-triggerexpression">{{t 'consoleTriggerExpression_delete_triggerexpression'}}</button>
    <form id="toolbar-form">
        <input type="text" name="keyword" class="toolbar-input" placeholder="{{t 'consoleTriggerExpression_triggerexpression_key'}}">
    </form>
    <button class="btn btn-info" id="toolbar-search">{{t 'consoleTriggerExpression_search'}}</button>
    <button class="btn" id="toolbar-reset">{{t 'consoleTriggerExpression_reset'}}</button>
</div>

<div id="triggerexpressionlist" class="triggerexpressionlist"></div>